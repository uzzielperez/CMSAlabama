  N_ED 4;
  M_S 4000;
}(model)

(processes){
  Process 21 21 -> 22 22;
  Scales VAR{Abs2(p[2]+p[3])};
  ME_Generator Internal;
  Loop_Generator gg_yy;
  End process;
  Process 93 93 -> 22 22;
  Order (*,2);
  CKKW sqr(20./E_CMS);
  End process;
}(processes)

(selector){
  Mass 22 22 500. 1000.;
  PT 22 70. E_CMS;
  PseudoRapidity 22 -2.8 2.8;
}(selector)
